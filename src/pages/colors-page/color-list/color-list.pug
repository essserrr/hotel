include color-entry/color-entry.pug
- var colors = [{title: 'Dark Shade 100%', description: "#1F2041", variant: "dark100"}, {title: 'Dark Shade 75%', description: "#1F2041", variant: "dark75"}, {title: 'Dark Shade 50%', description: "#1F2041", variant: "dark50"}, {title: 'Dark Shade 25%', description: "#1F2041", variant: "dark25"}, {title: 'Dark Shade 5%', description: "#1F2041", variant: "dark5"}, {title: 'Purple', description: "#BC9CFF", variant: "purple"}, {title: 'Green', description: "#6FCF97", variant: "green"}];

ul.color-list
    each value in colors
        li.color-list__entry
            +color-entry(value)
